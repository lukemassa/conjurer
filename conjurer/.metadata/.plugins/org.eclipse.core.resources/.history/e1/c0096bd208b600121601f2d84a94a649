// the Frame object
import java.util.*;

public class Frame {
	
	private String name;
	private LinkedList<Template> templates;
	
	public Frame(String s, HashMap<String,LinkedList<Template>> templates)
	{
		name = s;
		this.templates = templates.get(s);
		
	}
	
	public LinkedList<Template> getTemplates()
	{
		return templates;
	}
	
	public String toString()
	{
		return name;
	}
	
	public Frame getFrame(String verb, String filename)
	{
		Frame f = new Frame(verb);
		
		
		
		
		
	}
}
